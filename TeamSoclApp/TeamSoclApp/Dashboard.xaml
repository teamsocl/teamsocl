<Window x:Class="TeamSoclApp.Dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Dashboard" Height="512" Width="592.825">
    <Grid>
        <Button x:Name="Logout" Content="Log Out" HorizontalAlignment="Left" Margin="500,10,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Height="60"/>
        <TabControl HorizontalAlignment="Left" Height="373" Margin="160,99,0,0" VerticalAlignment="Top" Width="415">
            <TabItem Header="Messages">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Teams">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Team 1">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Team 2">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Team 3">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Team 4">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
