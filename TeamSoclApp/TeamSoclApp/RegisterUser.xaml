<Window x:Class="TeamSoclApp.RegisterUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RegisterUser" Height="663" Width="754" Icon="Teamsocl.ico">
    <Grid>
        <Button Content="Register" HorizontalAlignment="Left" Height="37" Margin="462,531,0,0" VerticalAlignment="Top" Width="146" Click="Register_Button"/>
        <TextBox x:Name="register_first_n" HorizontalAlignment="Left" Height="25" Margin="134,160,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="190" Text="first name"/>
        <TextBox x:Name="register_jersey_num" HorizontalAlignment="Left" Height="25" Margin="134,349,0,0" TextWrapping="Wrap" Text="jersey number" VerticalAlignment="Top" Width="190"/>
        <TextBox x:Name="register_phone" HorizontalAlignment="Left" Height="25" Margin="134,258,0,0" TextWrapping="Wrap" Text="phone number" VerticalAlignment="Top" Width="190"/>
        <TextBox x:Name="register_pwd" HorizontalAlignment="Left" Height="25" Margin="134,420,0,0" TextWrapping="Wrap" Text="password" VerticalAlignment="Top" Width="190"/>
        <TextBox x:Name="register_last_n" HorizontalAlignment="Left" Height="25" Margin="418,160,0,0" TextWrapping="Wrap" Text="last name" VerticalAlignment="Top" Width="190"/>
        <TextBox x:Name="register_nickname" HorizontalAlignment="Left" Height="25" Margin="134,210,0,0" TextWrapping="Wrap" Text="nickname" VerticalAlignment="Top" Width="190"/>
        <TextBox x:Name="register_email" HorizontalAlignment="Left" Height="25" Margin="134,302,0,0" TextWrapping="Wrap" Text="email" VerticalAlignment="Top" Width="190"/>
        <TextBox x:Name="register_pwd_confirm" HorizontalAlignment="Left" Height="25" Margin="418,420,0,0" TextWrapping="Wrap" Text="password confirm" VerticalAlignment="Top" Width="190"/>
        <CheckBox x:Name="register_privacy_opt" Content="Privacy Option&#xD;&#xA;&#xD;&#xA;Check this box if you want&#xD;&#xA;information sharing to be&#xD;&#xA;restricted to the Team &#xD;&#xA;Captain, rather than all &#xD;&#xA;teammates. " HorizontalAlignment="Left" Height="118" Margin="134,471,0,0" VerticalAlignment="Top" Width="184" IsThreeState="True"/>
        <TextBox x:Name="register_email_confirm" HorizontalAlignment="Left" Height="25" Margin="418,302,0,0" TextWrapping="Wrap" Text="email confirm" VerticalAlignment="Top" Width="190"/>

    </Grid>
</Window>
