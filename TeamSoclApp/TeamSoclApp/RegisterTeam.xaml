<Window x:Name="register_team_window" x:Class="TeamSoclApp.RegisterTeam"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Register Team" Height="399" Width="401" Icon="Teamsocl.ico">
    <Window.Background>
        <ImageBrush ImageSource="Team_Social.png"/>
    </Window.Background>
    <Grid Margin="0,0,0,-1">
        <Button x:Name="register_team_submit" Content="Submit" HorizontalAlignment="Left" Margin="249,321,0,0" VerticalAlignment="Top" Width="100" Height="25"/>
        <Button x:Name="register_team_cancel" Content="Cancel" HorizontalAlignment="Left" Margin="62,321,0,24" Width="100"/>
        <TextBox x:Name="register_team_name" HorizontalAlignment="Left" Height="25" Margin="182,257,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="167"/>
        <Label Content="Enter your Team Name" HorizontalAlignment="Left" Margin="30,257,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.184,0.269"/>

    </Grid>
</Window>
