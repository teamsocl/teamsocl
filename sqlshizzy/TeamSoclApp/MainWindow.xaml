<Window x:Class="TeamSoclApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="350" Width="525">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="258*"/>
            <ColumnDefinition Width="259*"/>
        </Grid.ColumnDefinitions>
        <Button Content="Login" HorizontalAlignment="Left" Margin="221,288,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" Grid.ColumnSpan="2" Height="22"/>
        <TextBox x:Name="EmailText" HorizontalAlignment="Left" Height="23" Margin="183,227,0,0" TextWrapping="Wrap" Text="Email" VerticalAlignment="Top" Width="152" Grid.ColumnSpan="2"/>
        <PasswordBox x:Name="PasswordText" HorizontalAlignment="Left" Margin="183,258,0,0" VerticalAlignment="Top" Width="151" Height="25" Grid.ColumnSpan="2"/>
        <Label Content="Email" HorizontalAlignment="Left" Margin="143,227,0,0" VerticalAlignment="Top" Height="26" Width="38"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="122,255,0,0" VerticalAlignment="Top" Height="26" Width="60"/>

    </Grid>
</Window>
